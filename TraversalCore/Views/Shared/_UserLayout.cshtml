
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="apple-touch-icon" sizes="76x76" href="~/traversalPanel/assets/img/apple-icon.png">
    <link rel="icon" type="image/png" href="~/traversalPanel/assets/img/favicon.png">
    <title>Üye Paneli</title>
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet" />
    <link href="~/traversalPanel/assets/css/nucleo-icons.css" rel="stylesheet" />
    <link href="~/traversalPanel/assets/css/nucleo-svg.css" rel="stylesheet" />
    <script src="https://kit.fontawesome.com/42d5adcbca.js" crossorigin="anonymous"></script>
    <link href="~/traversalPanel/assets/css/nucleo-svg.css" rel="stylesheet" />
    <link id="pagestyle" href="~/traversalPanel/assets/css/soft-ui-dashboard.css?v=1.0.3" rel="stylesheet" />
</head>

<body class="g-sidenav-show  bg-gray-100">

    @await Html.PartialAsync("~/Views/Shared/MemberLayoutPartial/_NavbarPartial.cshtml")

    <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">

        @await Html.PartialAsync("~/Views/Shared/MemberLayoutPartial/_HeaderPartial.cshtml")

        <div class="container-fluid py-4">

            @RenderBody()

            @await Html.PartialAsync("~/Views/Shared/MemberLayoutPartial/_FooterPartial.cshtml")

        </div>
    </main>

    @await Html.PartialAsync("~/Views/Shared/MemberLayoutPartial/_RigthbarPartial.cshtml")


    <script src="~/traversalPanel/assets/js/core/popper.min.js"></script>
    <script src="~/traversalPanel/assets/js/core/bootstrap.min.js"></script>
    <script src="~/traversalPanel/assets/js/plugins/perfect-scrollbar.min.js"></script>
    <script src="~/traversalPanel/assets/js/plugins/smooth-scrollbar.min.js"></script>
    <script>
        var win = navigator.platform.indexOf('Win') > -1;
        if (win && document.querySelector('#sidenav-scrollbar')) {
            var options = {
                damping: '0.5'
            }
            Scrollbar.init(document.querySelector('#sidenav-scrollbar'), options);
        }
    </script>
    <script async defer src="https://buttons.github.io/buttons.js"></script>
    <script src="~/traversalPanel/assets/js/soft-ui-dashboard.min.js?v=1.0.3"></script>
</body>

</html>
