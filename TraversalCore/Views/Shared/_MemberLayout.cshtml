<!DOCTYPE html>
<html class="loading" lang="en" data-textdirection="ltr">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="Chameleon Admin is a modern Bootstrap 4 webapp &amp; admin dashboard html template with a large number of components, elegant design, clean and organized code.">
    <meta name="keywords" content="admin template, Chameleon admin template, dashboard template, gradient admin template, responsive admin template, webapp, eCommerce dashboard, analytic dashboard">
    <meta name="author" content="ThemeSelect">
    <title>Traversal Üye Paneli</title>
    <link rel="apple-touch-icon" href="~/traversalMember/theme-assets/images/ico/apple-icon-120.png">
    <link rel="shortcut icon" type="image/x-icon" href="~/traversalMember/theme-assets/images/ico/favicon.ico">
    <link href="https://fonts.googleapis.com/css?family=Muli:300,300i,400,400i,600,600i,700,700i%7CComfortaa:300,400,700" rel="stylesheet">
    <link href="https://maxcdn.icons8.com/fonts/line-awesome/1.1/css/line-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/traversalMember/theme-assets/css/vendors.css">
    <link rel="stylesheet" type="text/css" href="~/traversalMember/theme-assets/css/app-lite.css">
    <link rel="stylesheet" type="text/css" href="~/traversalMember/theme-assets/css/core/menu/menu-types/vertical-menu.css">
    <link rel="stylesheet" type="text/css" href="~/traversalMember/theme-assets/css/core/colors/palette-gradient.css">
</head>
<body class="vertical-layout vertical-menu 2-columns   menu-expanded fixed-navbar" data-open="click" data-menu="vertical-menu" data-color="bg-gradient-x-purple-blue" data-col="2-columns">

    <!-- fixed-top-->
    @await Html.PartialAsync("~/Views/Shared/MemberLayoutPartial/_HeaderPartial.cshtml")
    <!-- ////////////////////////////////////////////////////////////////////////////-->

    @* @await Html.PartialAsync("~/Views/Shared/MemberLayoutPartial/_NavbarPartial.cshtml") *@
    <div class="main-menu menu-fixed menu-light menu-accordion    menu-shadow " data-scroll-to-active="true" data-img="~/traversalMember/theme-assets/images/backgrounds/02.jpg">
        <div class="navbar-header">
            <ul class="nav navbar-nav flex-row">
                <li class="nav-item mr-auto">
                    <a class="navbar-brand" href="index.html">
                        <img class="brand-logo" alt="Chameleon admin logo" src="~/traversalMember/theme-assets/images/logo/logo.png" />
                        <h3 class="brand-text">Chameleon</h3>
                    </a>
                </li>
                <li class="nav-item d-md-none"><a class="nav-link close-navbar"><i class="ft-x"></i></a></li>
            </ul>
        </div>
        <div class="main-menu-content">
            <ul class="navigation navigation-main" id="main-menu-navigation" data-menu="menu-navigation">
                <li class="active">
                    <a href="/Member/Dashboard/Index"><i class="ft-home"></i><span class="menu-title" data-i18n="">Dashboard</span></a>
                </li>
                <li class="nav-item">
                    <a href="/Default/Index"><i class="ft-home"></i><span class="menu-title" data-i18n="">@localizer["goToHome"]</span></a>
                </li>
                <li class="nav-item">
                    <a href="/Member/Profile/Index"><i class="ft-user"></i><span class="menu-title" data-i18n="">@localizer["myProfile"]</span></a>
                </li>
                <li class="nav-item">
                    <a href="/Member/Reservation/MyCurrentReservation"><i class="ft-calendar"></i><span class="menu-title" data-i18n="">@localizer["activeRez"]</span></a>
                </li>
                <li class="nav-item">
                    <a href="/Member/Reservation/MyOldReservation"><i class="ft-x-circle"></i><span class="menu-title" data-i18n="">@localizer["oldRez"]</span></a>
                </li>
                <li class="nav-item">
                    <a href="/Member/Reservation/MyApprovalReservation"><i class="ft-loader"></i><span class="menu-title" data-i18n="">@localizer["approvalRez"]</span></a>
                </li>
                <li class="nav-item">
                    <a href="/Member/Reservation/NewReservation"><i class="ft-plus"></i><span class="menu-title" data-i18n="">@localizer["newRez"]</span></a>
                </li>
                <li class="nav-item">
                    <a href="index.html"><i class="ft-log-out"></i><span class="menu-title" data-i18n="">@localizer["logout"]</span></a>
                </li>
            </ul>
        </div>
        <div class="navigation-background"></div>
    </div>


    <div class="app-content content">
        <div class="content-wrapper">
            <div class="content-wrapper-before"></div>
            @RenderBody()
        </div>
    </div>


    @await Html.PartialAsync("~/Views/Shared/MemberLayoutPartial/_FooterPartial.cshtml")

    <script src="~/traversalMember/theme-assets/vendors/js/vendors.min.js" type="text/javascript"></script>
    <script src="~/traversalMember/theme-assets/js/core/app-menu-lite.js" type="text/javascript"></script>
    <script src="~/traversalMember/theme-assets/js/core/app-lite.js" type="text/javascript"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    @RenderSection("scripts", false)
</body>
</html>